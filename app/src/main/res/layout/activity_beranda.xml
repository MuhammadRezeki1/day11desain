<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:background="@drawable/group_26"

    tools:context=".beranda">

    <ImageView
        android:id="@+id/src"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:src="@drawable/group_25"
        android:layout_marginLeft="70dp"
        android:layout_marginTop="30dp"
        />

    <ImageButton
        android:id="@+id/tvKoin"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:src="@drawable/group_16"
        android:layout_below="@id/src"
        android:layout_marginLeft="65dp"
        android:layout_marginTop="40dp"
        android:background="@android:color/transparent"
        />

    <ImageButton
        android:id="@+id/tvDana"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_below="@id/src"
        android:layout_marginLeft="160dp"
        android:layout_marginTop="40dp"
        android:src="@drawable/group_17"
        android:background="@android:color/transparent"
        />

    <ImageButton
        android:id="@+id/tvLive"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:src="@drawable/group_18"
        android:layout_below="@id/src"
        android:layout_marginLeft="250dp"
        android:layout_marginTop="30dp"
        android:background="@android:color/transparent"
        />

    <ImageButton
        android:id="@+id/pulsa"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_below="@id/src"
        android:layout_marginTop="130dp"
        android:layout_marginLeft="65dp"
        android:src="@drawable/group_19"
        android:background="@android:color/transparent"
        />

    <ImageButton
        android:id="@+id/free"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_below="@id/src"
        android:layout_marginLeft="160dp"
        android:layout_marginTop="130dp"
        android:src="@drawable/group_5"
        android:background="@android:color/transparent"
        />

    <ImageButton
        android:id="@+id/tvmall"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_below="@id/src"
        android:layout_marginTop="130dp"
        android:layout_marginLeft="250dp"
        android:src="@drawable/group_6"
        android:background="@android:color/transparent"
        />
    
    <ImageView
        android:id="@+id/tws"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:src="@drawable/group_9"
        android:layout_below="@id/pulsa"
        android:layout_marginTop="120dp"
        android:layout_marginLeft="45dp"
        />

    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_below="@id/pulsa"
        android:layout_marginTop="120dp"
        android:layout_marginLeft="120dp"
        android:src="@drawable/group_10"
        />

    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_below="@id/pulsa"
        android:layout_marginTop="120dp"
        android:layout_marginLeft="200dp"
        android:src="@drawable/group_11"
        />

    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_below="@id/pulsa"
        android:layout_marginTop="120dp"
        android:layout_marginLeft="280dp"
        android:src="@drawable/group_10"
        />

    <ImageView
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_below="@id/tws"
        android:layout_marginTop="100dp"
        android:layout_marginLeft="50dp"
        android:src="@drawable/group_13"
        />

    <ImageView
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_below="@id/tws"
        android:layout_marginTop="102dp"
        android:layout_marginLeft="150dp"
        android:src="@drawable/group_14"
        />

    <ImageView
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_below="@id/tws"
        android:layout_marginTop="100dp"
        android:layout_marginLeft="250dp"
        android:src="@drawable/group_15"
        />









</RelativeLayout>